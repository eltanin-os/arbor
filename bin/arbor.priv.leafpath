#!/bin/execlineb -S1
importas -i ARBOR ARBOR
cd $ARBOR
foreground {
	fdclose 2
	redo repo/packages.cdb
}
backtick -ED "" path { arbor.priv.cdbquery $1 repo/packages.cdb }
ifelse -Xn { test -n $path } { arbor.utils.err "package ${1} not found" }
pipeline {
	cd $path
	pwd -P
}
awk "{
	n=length(\"${ARBOR}\")
	print substr($0, n+2)
}"
