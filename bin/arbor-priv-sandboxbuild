#!/bin/execlineb -P
multisubstitute {
	importas -i ARBOR ARBOR
	importas -i ARBOR_HOSTARCH ARBOR_HOSTARCH
	importas -i ARBOR_HOSTOS ARBOR_HOSTOS
	importas -i ARBOR_PACKAGENAME ARBOR_PACKAGENAME
	importas -i ARBOR_SANDBOX ARBOR_SANDBOX
	importas -i ARBOR_TMPTARGETDIR ARBOR_TMPTARGETDIR
	importas -i ARBOR_TMPSYSTEMDIR ARBOR_TMPSYSTEMDIR
	importas -i PATH PATH
}
emptyenv -p
envfile "${ARBOR}/config/defaults"
export ARBOR "${ARBOR}"
export ARBOR_HOSTARCH "${ARBOR_HOSTARCH}"
export ARBOR_HOSTOS "${ARBOR_HOSTOS}"
export ARBOR_PACKAGENAME "${ARBOR_PACKAGENAME}"
export ARBOR_TMPTARGETDIR "${ARBOR_TMPTARGETDIR}"
export ARBOR_TMPSYSTEMDIR "${ARBOR_TMPSYSTEMDIR}"
#
export PATH "${ARBOR_TMPSYSTEMDIR}/bin:${PATH}"
arbor-priv-$ARBOR_SANDBOX
