#!/bin/execlineb -S1
ifelse -n { test -e "${1}" } { exit 1 }
multisubstitute {
	importas -is UNBZ2 UNBZ2
	importas -is UNTAR UNTAR
	importas -is UNGZ UNGZ
	importas -is UNLZ UNLZ
	importas -is UNXZ UNXZ
}
pipeline {
	case -- $1 {
		".*\.tar\.bz2|.*\.tbz2" { $UNBZ2 $1 }
		".*\.tar\.gz|.*\.tgz" { $UNGZ  $1 }
		".*\.tar\.lz|.*\.tlz" { $UNLZ  $1 }
		".*\.tar\.xz|.*\.txz" { $UNXZ  $1 }
	}
	arbor-utils-err "${1}: unknown format"
}
$UNTAR
